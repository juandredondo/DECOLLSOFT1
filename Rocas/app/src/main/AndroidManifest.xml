<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chino.rocas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".informacionClass"></activity>
        <activity android:name=".configurationClass"></activity>
        <activity android:name=".envioDatosClass"></activity>
        <activity android:name=".recibeDatosClass">
            <category android:name="android.intent.category.LAUNCHER" />
        </activity>

    </application>

</manifest>